/** Wyvern example: a Stack **/

interface Stack
	type T
	prop top : Int?
	meth push(element : Int)
	meth pop() : Int?
	
class StackImpl implements Stack
	var list : Link?
	
	meth top = list.?data
	
	meth push(element)
		list = Link(element, link)
	
	meth pop()
		val result = list.data
		list = list.next
		result
		
class Link
	type T
	val data : Int
	val next : Link?